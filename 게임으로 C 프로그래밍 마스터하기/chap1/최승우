#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#pragma warning(disable:4996)   // scanf를 무시한다는 에러가 뜨길래 인터넷에서 찾아서 붙혀주었따.


int main()
{
    int a;
    int b;
    int c;
    int d;
    int e;

    float f;
    int max;

    printf("input 2 data : ");
    scanf("%d %d", &a, &b);

    c = a + b;
    d = a - b;
    e = a * b;
    f = (float)a/b;

    if (a > b) {
        max = a;
    }
    else {
        max = b;
    }
    printf("%d + %d = %d\n",a, b, c);
    printf("%d - %d = %d\n",a, b, d);
    printf("%d * %d = %d\n",a, b, e);
    printf("%d / %d = %f\n",a, b, f);
    printf("max : %d\n", max);

    return 0;
}
